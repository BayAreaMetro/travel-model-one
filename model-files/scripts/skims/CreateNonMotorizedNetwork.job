   if (a = 33897 & b = 33886 ) WALKOK = 1
   if (a = 33898 & b = 33933 ) WALKOK = 1
   if (a = 33899 & b = 33897 ) WALKOK = 1
   if (a = 33933 & b = 33934 ) WALKOK = 1
   if (a = 33934 & b = 33953 ) WALKOK = 1
   if (a = 33952 & b = 33899 ) WALKOK = 1
