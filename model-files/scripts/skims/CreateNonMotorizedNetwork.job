   if (a = 32783 & b = 6972 ) WALKOK = 1
   if (a = 6972 & b = 6968 ) WALKOK = 1
   if (a = 6968 & b = 6970 ) WALKOK = 1
   if (a = 6971 & b = 6969 ) WALKOK = 1
   if (a = 6969 & b = 6973 ) WALKOK = 1
   if (a = 6973 & b = 32784 ) WALKOK = 1
   if (a = 32784 & b = 32802 ) WALKOK = 1
   if (a = 32802 & b = 32787 ) WALKOK = 1
        
endrun
