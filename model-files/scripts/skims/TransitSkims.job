              ELSEIF (MI.3.@token_ivt@>0)
                MW[1 ] =MI.3.ivt      
                MW[2 ] =MI.3.iwait    
                MW[3 ] =MI.3.xwait   
                MW[4 ] =MI.3.wait   
                MW[5 ] =MI.3.wacc   
                MW[6 ] =MI.3.waux   
                MW[7 ] =MI.3.wegr   
                MW[8 ] =MI.3.dtime   
                MW[9 ] =MI.3.ddist   
