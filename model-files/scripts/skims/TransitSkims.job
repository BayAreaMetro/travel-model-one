                MW[11] =MI.3.boards  
                MW[12] =MI.3.ivtLOC   
                MW[13] =MI.3.ivtLRF   
                MW[14] =MI.3.ivtEXP   
                MW[15] =MI.3.ivtHVY   
                MW[16] =MI.3.ivtCOM   
